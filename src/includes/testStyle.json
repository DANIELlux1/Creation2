{
    "styles": 
    [
        {
            "class": "*",
            "style": 
            [
                "margin: 0;",
                "padding: 0;",
                "box-sizing: border-box;"
            ]
        },
        {
            "class": "html",
            "style": 
            [
                "color: #555;",
                "font-family: 'Lato', sans-serif;",
                "font-weight: 300;",
                "font-size: 20px;",
                "text-rendering: optimizeLegibility;"
            ]
        },
        {
            "class": ".clearfix:after",
            "style": 
            [
                "content: '.';",
                "clear: both;",
                "display: block;",
                "height: 0;",
                "visibility: hidden;"
            ]
        },
        {
            "class": ".addFrame",
            "style": 
            [
                "margin-top: 40px;",
                "width: 100%;",
                "text-align: center;"
            ]
        },
        {
            "class": ".page",
            "style": 
            [
                "max-width: 80%;",
                "margin: 0 auto 0 auto;"
            ]
        },
        {
            "class": ".frame",
            "style": 
            [
                "width: 100%;",
                "height: 600px;",
                "overflow: auto;"
            ]
        },
        {
            "class": ".panel",
            "style": 
            [
                "float: left;",
                "width: 50%;",
                "height: 270px;",
                "display: block;"
            ]
        },
        {
            "class": ".content",
            "style": 
            [
                "max-width: 100%;",
                "max-height: 100%;"
            ]
        },
        {
            "class": "p",
            "style": 
            [
                "margin-top: 20px;",
                "font-size: 90%;",
                "line-height: 145%;",
                "text-align: justify;"
            ]
        },
        {
            "class": ".panel .content span img",
            "style": 
            [
                "max-width: auto;",
                "max-height: 200px;",
                "display: block;"
            ]
        },
        {
            "class": ".page-default-4-panel",
            "style": 
            [
                "background-color: red;"
            ]
        },
        {
            "class": ".default-title-header",
            "style": 
            [
                "background-color: green;"
            ]
        }
    ]
}